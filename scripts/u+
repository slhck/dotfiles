#!/usr/bin/env python3
# Lookup Unicode character by hex code (e.g., 'u+ 2025' -> 'Ã±, LATIN SMALL LETTER N WITH TILDE')
import argparse
import unicodedata


def main():
    parser = argparse.ArgumentParser()
    parser.add_argument("hex_number", type=str)
    args = parser.parse_args()

    as_int = int(args.hex_number, 16)
    as_chr = chr(as_int)

    print(as_chr)
    print(unicodedata.name(as_chr))


if __name__ == "__main__":
    main()
